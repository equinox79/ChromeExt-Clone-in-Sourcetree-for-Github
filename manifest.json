{
    "name": "Clone in Sourcetree for Github",
    "version": "0.0.1",
    "manifest_version": 1,
    "description": "Show 'Clone in Sourcetree' Button on Github.",
    "content_scripts": [
        {
          "matches": ["https://github.com/*"],
          "js": ["vendor/js/jquery-1.9.1.min.js", "vendor/js/underscore-min.js", "js/script.js"],
          "run_at": "document_start"
        }
    ]
}
